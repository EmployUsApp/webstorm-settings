<application>
  <component name="JSEditorOptions">
    <option name="START_STRING_INTERPOLATION_ON_TYPING" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="\bhack\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="7ab00" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bwarning\b.*">
      <option name="FOREGROUND" value="0" />
      <option name="BACKGROUND" value="b70f14" />
      <option name="FONT_TYPE" value="1" />
      <option name="ERROR_STRIPE_COLOR" value="ab0700" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="\bugly\b.*">
      <option name="FOREGROUND" value="bd4800" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <filter name="To Do">
      <pattern index="1" />
      <pattern index="0" />
    </filter>
    <filter name="To Improve">
      <pattern index="1" />
      <pattern index="4" />
      <pattern index="2" />
    </filter>
    <filter name="Bad">
      <pattern index="3" />
      <pattern index="2" />
    </filter>
  </component>
</application>